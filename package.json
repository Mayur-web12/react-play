{
  "name": "react-play",
  "version": "1.0.0",
  "private": true,
  "scripts": {
	  "start:lint": "npm run lint && react-scripts start",
    "vite:build": "vite build --mode production",
    "vite:start": "npx kill-port 3000 && clear && vite --mode development --host --force --config vite.config.ts",
    "vite:prod": "tsc && vite build --mode production --host",
    "craco:build": "craco build",
    "craco:test": "craco test",
    "craco:start": "npx kill-port 3000 && craco start",
    "start": "npx kill-port 3000 && react-scripts start",
    "build": "npx --yes create-react-play@latest -p && react-scripts build",
    "snap": "react-snap",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "preinstall": "npm run createplay",
    "createplay": "npx --yes create-react-play@latest -p",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:all": "eslint --fix src/**/*.{css,scss,json,jsx,js,ts,tsx}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx}",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,css,md}\" --config ./.prettierrc",
    "prepare": "husky install"
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@giscus/react": "^2.0.3",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.9.1",
    "@mui/styles": "^5.9.3",
    "@nhost/react": "^0.9.0",
    "@reduxjs/toolkit": "^1.9.2",
    "axios": "^1.3.3",
    "date-fns": "^2.28.0",
    "git-repo-api": "^0.0.17",
    "graphql": "^16.5.0",
    "json-graphql-parser": "^0.1.8",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-countdown": "^2.3.5",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.8.0",
    "react-loader-spinner": "~5.1.5",
    "react-redux": "^8.0.1",
    "react-refresh": "^0.14.0",
    "react-router-dom": "^6.4.0",
    "react-shimmer-effect": "^1.0.9",
    "react-toastify": "^9.0.8",
    "react-twitter-widgets": "^1.11.0",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "url": "^0.11.0",
    "web-vitals": "^2.1.0",
    "workbox-cacheable-response": "^6.5.4",
    "workbox-core": "^6.5.4",
    "workbox-expiration": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4",
    "workbox-window": "^6.5.4"
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@craco/types": "^7.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.7",
    "craco-esbuild": "^0.5.2",
    "cypress": "^10.6.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "postcss": "^8.4.13",
    "prettier": "^2.7.1",
    "puppeteer": "^13.7.0",
    "react-scripts": "^5.0.0",
    "react-snap": "^1.23.0",
    "sass": "^1.62.1",
    "stylelint": "^15.6.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^9.0.0",
    "tailwind-scrollbar": "^2.1.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^5.0.4",
    "vite": "4.2.1",
    "vite-plugin-chunk-split": "^0.4.7",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-pwa": "^0.14.7",
    "vite-plugin-react-esbuild": "^0.1.2",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.7",
    "webpack": "^5.74.0",
    "webpack-merge": "^5.8.0"
  },
  "cracoConfig": "config/craco.config.js",
  "lint-staged": {
    "{src,test}/**/*.{css,scss,json,jsx,js,ts,tsx}": [
      "prettier --write -u"
    ],
    "{src,test}/**/*.{jsx,js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "pnpm": {
    "overrides": {
      "redux": "$redux",
      "date-fns": "$date-fns",
      "react-scripts": "$react-scripts",
      "webpack": "$webpack",
      "webpack-merge": "$webpack-merge"
    }
  }
}
